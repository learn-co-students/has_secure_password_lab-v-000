sessions/new <br>
<h1>Log In</h1>

<%= form_tag '/login' do %>
  <%= label_tag "Name" %>
  <%= text_field_tag :name %>

  <%= label_tag "Password" %>
  <%= text_field_tag :password %>

  <%= submit_tag 'Log In' %>
<% end %>

<br>
<p>Don't have an account?</p>
<%= link_to 'Register!', new_user_path %>
