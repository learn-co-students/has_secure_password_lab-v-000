<%= form_for :user, url: login_path, method: :post do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.submit "Login" %>
<% end %>
or
<%= form_tag new_user_path, method: :get do %>
<%= submit_tag "Create User" %>
<% end %>
