users/new <br>
<h1>Register</h1>

<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit 'Register' %>
<% end %>

<br>
<p>Already have an account?</p>
<%= link_to 'Log In!', login_path %>
