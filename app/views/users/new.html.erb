<%= form_for :user, controller: 'users', action: 'create' do |f| %>
  <%= f.label "Username" %>
  <%= f.text_field :name %>

  <%= f.label "Password" %>
  <%= f.password_field :password %>

  <%= f.label "Confirm Password" %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Submit" %>
<% end %>
