<h1>User#new</h1>
<p>Find me in app/views/user/new.html.erb</p>

<h2>Sign up</h2>
<%= form_for(@user) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <br>
    <%= f.label :password_confirmation  %>
    <%= f.text_field :password_confirmation  %>
    <br>
    <%= f.submit %>
<% end %>