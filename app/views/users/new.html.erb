New User Signup!

<%= form_for User.new do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :password %>
    <%= f.password_field :password %>


    <%= f.label "confirm your password" %>
    <%= f.password_field :password_confirmation %>
    <%= f.submit "signup"%>

<% end %>