<h1>Sign Up Here:</h1>

<%= form_for :user do |f| %>
  <label>User Name:</label>
  <%= f.text_field :'name' %>
  <label>Password:</label>
  <%= f.password_field :'password' %>
  <label>Password Confirmation:</label>
  <%= f.password_field :'password_confirmation' %>
  <%= f.submit "Submit" %>
<% end %>
