<h1> Brand new? Sign up here: </h1>

<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label 'Password Confirmation' %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit "Sign Up" %>
<% end %>
