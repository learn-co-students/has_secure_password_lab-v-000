<h1>Create a New User:</h1>
<br>
<%= form_for :user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<br>
<%= f.submit "Create" %>
<% end %>
