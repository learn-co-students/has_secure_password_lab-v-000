<h1> Sign up here! </h1>

<%= form_for :user, url: '/users' do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %>

   <%= f.label :password %>
   <%= f.password_field :password %>

   <%= f.label :password %>
   <%= f.password_field :password_confirmation %>

   <%= f.submit "Sign Up" %>
<% end %>
