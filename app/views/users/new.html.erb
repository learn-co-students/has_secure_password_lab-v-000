<%= form_for :user, url: users_path, :method => 'POST' do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation, 'Confirmation' %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit "Sign Up" %>

<% end %>
